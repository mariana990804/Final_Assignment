@article{shanmuganathanCrossPlatformMetabolomicsComparison2021,
	title = {A {Cross}-{Platform} {Metabolomics} {Comparison} {Identifies} {Serum} {Metabolite} {Signatures} of {Liver} {Fibrosis} {Progression} in {Chronic} {Hepatitis} {C} {Patients}},
	volume = {8},
	issn = {2296-889X},
	url = {https://www.frontiersin.org/journals/molecular-biosciences/articles/10.3389/fmolb.2021.676349/full},
	doi = {10.3389/fmolb.2021.676349},
	abstract = {{\textless}p{\textgreater}Metabolomics offers new insights into disease mechanisms that is enhanced when adopting orthogonal instrumental platforms to expand metabolome coverage, while also reducing false discoveries by independent replication. Herein, we report the first inter-method comparison when using multisegment injection-capillary electrophoresis-mass spectrometry (MSI-CE-MS) and nuclear magnetic resonance (NMR) spectroscopy for characterizing the serum metabolome of patients with liver fibrosis in chronic hepatitis C virus (HCV) infection ({\textless}italic{\textgreater}n{\textless}/italic{\textgreater} = 20) and non-HCV controls ({\textless}italic{\textgreater}n{\textless}/italic{\textgreater} = 14). In this study, 60 and 30 serum metabolites were detected frequently (\&gt;75\%) with good technical precision (median CV \&lt; 10\%) from serum filtrate samples ({\textless}italic{\textgreater}n{\textless}/italic{\textgreater} = 34) when using standardized protocols for MSI-CE-MS and NMR, respectively. Also, 20 serum metabolite concentrations were consistently measured by both methods over a 500-fold concentration range with an overall mean bias of 9.5\% ({\textless}italic{\textgreater}n{\textless}/italic{\textgreater} = 660). Multivariate and univariate statistical analyses independently confirmed that serum choline and histidine were consistently elevated ({\textless}italic{\textgreater}p{\textless}/italic{\textgreater} \&lt; 0.05) in HCV patients with late-stage (F2-F4) as compared to early-stage (F0-F1) liver fibrosis. Overall, the ratio of serum choline to uric acid provided optimal differentiation of liver disease severity ({\textless}italic{\textgreater}AUC{\textless}/italic{\textgreater} = 0.848, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater} = 0.00766) using a receiver operating characteristic curve, which was positively correlated with liver stiffness measurements by ultrasound imaging ({\textless}italic{\textgreater}r{\textless}/italic{\textgreater} = 0.606, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater} = 0.0047). Moreover, serum 5-oxo-proline concentrations were higher in HCV patients as compared to non-HCV controls ({\textless}italic{\textgreater}F{\textless}/italic{\textgreater} = 4.29, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater} = 0.0240) after adjustment for covariates (age, sex, BMI), indicative of elevated oxidative stress from glutathione depletion with the onset and progression of liver fibrosis. Both instrumental techniques enable rapid yet reliable quantification of serum metabolites in large-scale metabolomic studies with good overlap for biomarker replication. Advantages of MSI-CE-MS include greater metabolome coverage, lower operating costs, and smaller sample volume requirements, whereas NMR offers a robust platform supported by automated spectral and data processing software.{\textless}/p{\textgreater}},
	language = {English},
	urldate = {2025-01-07},
	journal = {Frontiers in Molecular Biosciences},
	author = {Shanmuganathan, Meera and Sarfaraz, Mohammad Omair and Kroezen, Zachary and Philbrick, Holly and Poon, Richel and Don-Wauchope, Andrew and Puglia, Marco and Wishart, David and Britz-McKibbin, Philip},
	month = aug,
	year = {2021},
	note = {Publisher: Frontiers},
	keywords = {biomarkers, CE-MS, hepatitis C (HCV) infection, liver fibrosis (LF), Metabolomics (OMICS), NMR, Serum},
	file = {Full Text PDF:C\:\\Users\\user1\\Zotero\\storage\\79247WR5\\Shanmuganathan et al. - 2021 - A Cross-Platform Metabolomics Comparison Identifies Serum Metabolite Signatures of Liver Fibrosis Pr.pdf:application/pdf},
}
