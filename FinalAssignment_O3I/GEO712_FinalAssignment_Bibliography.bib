	number = {11},
	urldate = {2025-01-07},
	journal = {Journal of Lipid Research},
	author = {Ly, Ritchie and MacIntyre, Brittany C. and Philips, Stuart M. and McGlory, Chris and Mutch, David M. and Britz-McKibbin, Philip},
